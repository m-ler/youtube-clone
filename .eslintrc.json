{
	"extends": [
		"next/core-web-vitals",
		"prettier",
		"plugin:@typescript-eslint/recommended"
	],
	"parser": "@typescript-eslint/parser",
	"rules": {
		"@typescript-eslint/no-non-null-assertion": "off",
		"react-hooks/exhaustive-deps": 0,
		"prefer-spread": "off",
		"no-unused-vars": "off",
		"@typescript-eslint/no-unused-vars": "warn"
	}
}